{
    "name": "Examle 1",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "sample",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["content.bundle.js"]
        }
    ],
    "browser_action": {
        "default_popup": "pupup.html"
    },
    "background": {
        "scripts": ["background.bundle.js"],
        "persistant": false
    },
    "web_accessible_resources": ["callback.html"],
    "permissions": [
        "tabs",
        "https://sandbox.evernote.com/*",
        "https://evernote.com/*"
    ]
}
